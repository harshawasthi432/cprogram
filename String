//sum of odd number and even numbers
#include <stdio.h>
#include<string.h> 
int main()
{
    int arr[100],n,i,even=0,odd=0;
    printf("PRINT HOW MANY NUMBER");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        printf("enter array");
        scanf("%d",&arr[i]);
    
    if(arr[i]%2==0)
    {
        even=even+arr[i];
    }
    else
    {
        odd=odd+arr[i];
    }
}
printf("sum of even no is %d",even);
printf("\nsum of odd number is %d",odd);
return 0;
}
